# -*- mode: ruby -*-
# vi: set ft=ruby :

Vagrant.configure(2) do |config|
    config.vm.define "my-debian" do |box|
        box.vm.box = "debian/jessie64"
        box.vm.hostname = "my-debian"
        box.vm.provider :virtualbox do |provider|
            provider.name = "my-debian"
            provider.memory = 512
            provider.cpus = 1
        end
    end
end
