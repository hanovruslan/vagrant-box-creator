# -*- mode: ruby -*-
# vi: set ft=ruby :

Vagrant.configure(2) do |config|
    config.vm.define "default" do |machine|
        machine.vm.box = "debian/jessie64"
        machine.vm.hostname = "default"
        machine.vm.provider :virtualbox do |provider|
            provider.name = "default"
            provider.memory = 512
            provider.cpus = 1
        end
    end
end
