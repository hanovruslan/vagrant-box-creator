# -*- mode: ruby -*-
# vi: set ft=ruby :

Vagrant.configure(2) do |config|
    config.vm.define "my-debian" do |box|
        box.vm.box = "my-debian"
        box.vm.hostname = "my-debian"
        box.vm.provider :virtualbox do |provider|
            provider.name = "my-debian"
        end
        box.vm.network :private_network, type: "dhcp"
    end
end
