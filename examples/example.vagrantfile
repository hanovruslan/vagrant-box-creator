# -*- mode: ruby -*-
# vi: set ft=ruby :

Vagrant.configure(2) do |config|
    config.vm.define "example" do |box|
        box.vm.box = "example"
        box.vm.hostname = "example"
        box.vm.provider :virtualbox do |provider|
            provider.name = "example"
        end
        box.vm.network :private_network, type: "dhcp"
    end
end
